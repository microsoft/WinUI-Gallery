<?xml version="1.0" encoding="utf-8" ?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style
        x:Key="OutputTextBlockStyle"
        BasedOn="{StaticResource BodyTextBlockStyle}"
        TargetType="TextBlock">
        <Setter Property="Margin" Value="8,8,0,0" />
    </Style>
    <Style
        x:Key="SubtitleHeadingTextBlockStyle"
        BasedOn="{StaticResource SubtitleTextBlockStyle}"
        TargetType="TextBlock">
        <Setter Property="AutomationProperties.HeadingLevel" Value="Level2" />
    </Style>
    <Style
        x:Key="BodyStrongHeadingTextBlockStyle"
        BasedOn="{StaticResource BodyStrongTextBlockStyle}"
        TargetType="TextBlock">
        <Setter Property="AutomationProperties.HeadingLevel" Value="Level3" />
    </Style>
</ResourceDictionary>
